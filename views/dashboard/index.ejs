<% include ../layout/header %>
<div class="fix-left-side left-side-menu"> 
    <% include ../layout/logo-section %> 
    <% include ../layout/left-menu %> 
</div>
<% include ../layout/header-right-menu %>
</div>
<div class="content-wrapper">
    <div class="page-name"><div class="col-lg-12 col-xs-12"><div class="dashboard">My Dashboard</div> </div></div>
    <div class="sortable">
        <div class="col-md-12"> 
            <div>
                <div class="panel">
                    <header class="panel-heading">
                        Current shipment 
                        <span class="tool-bar pull-right"> 
                            <a class="fa fa-external-link" href="javascript:;"></a> 
                            <a class="fa fa-chevron-down" href="javascript:;"></a> 
                        </span> 
                        <div class="pull-right tool-bar">
                            <input type="hidden" id="minDate">
                            <input type="hidden" id="maxDate">
                            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#addShipping">Add Shipping <i class="fa fa-plus"></i></button>
                            <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#uploadShipping">Upload File <i class="fa fa-upload"></i></button>
                            <button type="button" onclick="window.location.href='<%=assetsLocation%>/templates/template.xlsx'" class="btn btn-primary">Template File <i class="fa fa-download"></i></button>
                        </div>
                    </header>
                    <div class="panel-body">
                        <% if(messages.success) { %>                                             
                            <div class="alert alert-success"><strong>Thank You ! </strong><%=messages.success %></div>
                        <% }%>
                        <div class="adv-table editable-table ">
                            <% include jobs-grid %>  
                        </div>        
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="uploadShipping" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <!-- Upload Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title"><i class="fa fa-upload"></i> Upload File</h4>
            </div>
            <form action="/dashboard/upload-shipping" method="POST" enctype="multipart/form-data">
                <div class="modal-body">
                    <input type="file" id="shipping_file" name="shipping_file">
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary">Upload</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </form>
        </div>
    </div>
</div>
<div id="addShipping" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <!-- Add/Edit Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title"><i class="fa fa-pencil-square"></i> Add/Edit Shipping Info</h4>
            </div>
            <form action="/dashboard/add-shipping" id="addShippingForm" method="POST">
                <div class="modal-body">
                    <div class="form-group ">
                        <label class="control-label" for="cnee">CNEE</label>
                        <input class="form-control valid validate[required]" id="cnee" data-errormessage-value-missing="cnee is required." name="cnee"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="line">Line</label>
                        <input class="form-control valid validate[required]" id="line" data-errormessage-value-missing="Line is required." name="line"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="origin">Origin</label>
                        <input class="form-control valid validate[required]" id="origin" data-errormessage-value-missing="Origin is required." name="origin"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="job_no">Job Number</label>
                        <input class="form-control valid validate[required]" id="job_no" data-errormessage-value-missing="Job number is required." name="job_no"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="type">Type</label>
                        <input class="form-control valid validate[required]" id="type" data-errormessage-value-missing="Type is required." name="type"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="contact_no">Contact Number</label>
                        <input class="form-control valid validate[required]" id="contact_no" data-errormessage-value-missing="Contact number is required." name="contact_no"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="pkgs">PKGS</label>
                        <input class="form-control valid validate[required]" id="pkgs" data-errormessage-value-missing="pkgs is required." name="pkgs"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="weight">Weight</label>
                        <input class="form-control valid validate[required]" id="weight" data-errormessage-value-missing="Weight is required." name="weight"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="vol">Volume</label>
                        <input class="form-control valid validate[required]" id="vol" data-errormessage-value-missing="Volume is required." name="vol"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="hbl">HBL</label>
                        <input class="form-control valid validate[required]" id="hbl" data-errormessage-value-missing="hbl is required." name="hbl"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="etd">Estimated Date</label>
                        <input class="form-control valid validate[required] datepicker" id="etd" data-errormessage-value-missing="Estimated date is required." name="etd"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="nhs">NHS</label>
                        <input class="form-control valid validate[required] datepicker" id="nhs" data-errormessage-value-missing="nhs is required." name="nhs"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="shipper">Shipper</label>
                        <input class="form-control valid validate[required]" id="shipper" data-errormessage-value-missing="Shipper is required." name="shipper"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="mb_l">MB/L</label>
                        <input class="form-control valid validate[required]" id="mb_l" data-errormessage-value-missing="MB/L is required." name="mb_l"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="agent">Agent</label>
                        <input class="form-control valid validate[required]" id="agent" data-errormessage-value-missing="Agent is required." name="agent"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="igm">IGM</label>
                        <input class="form-control valid validate[required]" id="igm" data-errormessage-value-missing="IGM is required." name="igm"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="zoho_freight">Zoho Freight</label>
                        <input class="form-control valid validate[required]" id="zoho_freight" data-errormessage-value-missing="Zoho Freight is required." name="zoho_freight"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="zoho_line">Zoho Line</label>
                        <input class="form-control valid validate[required]" id="zoho_line" data-errormessage-value-missing="Zoho Line is required." name="zoho_line"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="ohbl">OHBL</label>
                        <input class="form-control valid validate[required]" id="ohbl" data-errormessage-value-missing="OHBL is required." name="ohbl"  type="text" >
                    </div>
                    <div class="form-group ">
                        <label class="control-label" for="payment">Payment</label>
                        <input class="form-control valid validate[required]" id="payment" data-errormessage-value-missing="Payment is required." name="payment"  type="text" >
                    </div>
                    <div class="form-group ">
                        <input class="form-control" id="shipping_id" name="id" type="hidden">
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="submit" class="btn btn-primary">Sumbit</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </form>
        </div>
    </div>
</div>
<% include ../layout/footer %>