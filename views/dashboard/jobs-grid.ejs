    <div class="row">
        <div class="col-md-5">
            <div class="btn-group" id="for-date-range-filter">
                <div class="btn btn-default dropdown-toggle selected-date-range" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span id="from" class="date_range_filter">22/March/2018</span> to <span id="to" class="date_range_filter">23/March/2018</span>
                    <span class="caret"></span>
                    <input type="hidden" name="min" id="minDate">
                    <input type="hidden" name="min" id="maxDate">
                </div>
                <div class="dropdown-menu" style="padding:10px;width:100%;" id="pickerContainer">
                    <div class="tab-content">
                        <div class="tab-pane active" id="custom_datepicker">
                            <br>
                                <div class="form-group">
                                    <label for="exampleInputEmail2">Date Range :</label>
                                    <div class="col-md-12 input-group input-append date" id="datePicker">
                                        <input readonly type="text" class="form-control" name="date_from" id="date_from">
                                        <span class="input-group-addon add-on">
                                            <span class="glyphicon glyphicon-calendar text-danger"></span>
                                        </span>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="exampleInputEmail2">To</label>
                                    <div class="col-md-12 input-group input-append date" id="datePicker1">
                                        <input readonly type="text" class="form-control" name="date_to" id="date_to">
                                        <span class="input-group-addon add-on">
                                            <span class="glyphicon glyphicon-calendar text-danger"></span>
                                        </span>
                                    </div>
                                </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-7">
            <div class="clearfix add-new pull-right" id="dataTableButtons">
                
            </div>
        </div>
    </div>
    <table class="table table-striped table-hover table-bordered display" id="JobsGrid">
        <thead>
            <tr>
                <th>#</th>
                <th>CNEE</th>
                <th>Line</th>
                <th>Origin</th>
                <th>Job No</th>
                <th>Type</th>
                <th>Contact No</th>
                <th>PKGS</th>
                <th>Weight</th>
                <th>Volume</th>
                <th>HBL</th>
                <th>Estimated Date</th>
                <th>NHS</th>
                <th>Shipper</th>
                <th>MB/L</th>
                <th>Agent</th>
                <th>IGM</th>
                <th>Zoho Freight</th>
                <th>Zoho Line</th>
                <th>OHBL</th>
                <th>Payment</th>
                <th>fff</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            <% var i=1; data.forEach(function(ship){ %>
                <tr class="odd" role="row">  
                    <td><%= i %></td>      
                    <td><%= ship.cnee %></td>                            
                    <td><%= ship.line %></td>
                    <td><%= ship.origin %></td>
                    <td><%= ship.job_no %></td>
                    <td><%= ship.type %></td>
                    <td><%= ship.contact_no %></td>
                    <td><%= ship.pkgs %></td>
                    <td><%= ship.weight %></td>
                    <td><%= ship.vol %></td>
                    <td><%= ship.hbl %></td>
                    <td><%= moment(ship.etd).format('DD/MM/YYYY') %></td>
                    <td><%= moment(ship.nhs).format('DD/MM/YYYY') %></td>
                    <td><%= ship.shipper %></td>
                    <td><%= ship.mb_l %></td>
                    <td><%= ship.agent %></td>
                    <td><%= ship.igm %></td>
                    <td><%= ship.zoho_freight %></td>
                    <td><%= ship.zoho_line %></td>
                    <td><%= ship.ohbl %></td>
                    <td><%= ship.payment %></td>
                    <td><%= moment(ship.etd).format('YYYYMMDD') %></td>
                    <td><a href="javascript:void(0)" onclick="openModel('<%= ship.id %>')" ><i class="fa fa-2x fa-pencil-square-o"></i></a></td>  
                </tr>
            <% i++; }) %>

        </tbody>
    </table>